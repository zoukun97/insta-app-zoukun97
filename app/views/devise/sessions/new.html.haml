= image_tag 'Rectangle.png', alt: '', height: '', width: '', class: ''
.container
  = form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
    .field
      = f.label :email
      %br/
      = f.email_field :email, autofocus: true, autocomplete: "email", class: 'form'
    .field
      = f.label :password
      %br/
      = f.password_field :password, autocomplete: "current-password", class: 'form'
    - if devise_mapping.rememberable?
      .field
        = f.check_box :remember_me
        = f.label :remember_me
    .actions
      = f.submit "LOGIN", class: 'btn'
  = render "devise/shared/links"
